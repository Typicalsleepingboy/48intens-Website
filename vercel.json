{
  "version": 2,
  "redirects": [
    {
      "source": "https://www.48intens.com/:path*",
      "destination": "https://48intens.com/:path*",
      "permanent": true
    }
  ],

  "rewrites": [
    {
      "source": "/",
      "destination": "/home.html"  
    },
    {
      "source": "/:path*",
      "destination": "/:path*.html" 
    },
    {
      "source": "/css/:path*",   
      "destination": "/css/:path*"
    },
    {
      "source": "/:css*.css",
      "destination": "/:css*.css"
    }
  ]
}