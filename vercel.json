{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/welcome.html"
    },
{
      "source": "/:path(?!discord)",
      "destination": "/:path*.html"
    }
  ],
  "redirects": [
    {
      "source": "/discord",
      "destination": "https://48intens.com/discord",
      "permanent": true
    }
  ]
}
